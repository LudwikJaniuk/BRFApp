{
  "name": "brf-energi",
  "version": "2.4.1",
  "description": "Brf Energi webapp",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "restart": "mongod --dbpath data --fork --logpath /dev/null && nodemon -r dotenv/config --inspect index.js -i lib/components && npm stop",
    "stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "test": "standard && stylelint **/*.css",
    "postmerge": "bin/setup",
    "postinstall": "bin/setup",
    "postbuild": "npm run compress",
    "compress:js:app": "gzip -9 -f -c public/index-$npm_package_version.js > public/index-$npm_package_version.js.gz",
    "compress:js:mapbox": "gzip -9 -f -c public/mapbox-gl.js > public/mapbox-gl.js.gz",
    "compress:js:highcharts": "npm run compress:js:highcharts:main && npm run compress:js:highcharts:no-data",
    "compress:js:highcharts:main": "gzip -9 -f -c public/highcharts.js > public/highcharts.js.gz",
    "compress:js:highcharts:no-data": "gzip -9 -f -c public/highcharts/modules/no-data-to-display.js > public/highcharts/modules/no-data-to-display.js.gz",
    "compress:js": "npm run compress:js:app && npm run compress:js:mapbox && npm run compress:js:highcharts",
    "compress:css": "gzip -9 -f -c public/index-$npm_package_version.css > public/index-$npm_package_version.css.gz",
    "compress": "npm run compress:css && npm run compress:js",
    "build:js": "npm run build:js:app && npm run build:js:mapbox && npm run build:js:highcharts",
    "build:js:app": "browserify lib/app/index.js -t localenvify -t yo-yoify -t babelify -g unassertify -g yo-yoify -g uglifyify | uglifyjs -o public/index-$npm_package_version.js",
    "build:js:mapbox": "browserify -r mapbox-gl | uglifyjs -o public/mapbox-gl.js",
    "build:js:highcharts": "npm run build:js:highcharts:main && npm run build:js:highcharts:no-data",
    "build:js:highcharts:main": "browserify -r highcharts | uglifyjs -o public/highcharts.js",
    "build:js:highcharts:no-data": "browserify -r highcharts/modules/no-data-to-display | uglifyjs -o public/highcharts/modules/no-data-to-display.js",
    "build:css": "postcss lib/app/index.css --no-map -u postcss-import -u postcss-custom-media -u postcss-custom-properties -u autoprefixer -u postcss-csso -o public/index-$npm_package_version.css",
    "build": "npm run build:css && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CIVIS-project/BRFApp"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-plugin-stack-trace-sourcemap": "^1.0.2",
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chokidar": "^1.7.0",
    "choo-devtools": "^2.1.0",
    "eslint": "^3.19.0",
    "husky": "^0.14.3",
    "localenvify": "^1.0.1",
    "nodemon": "^1.4.0",
    "postcss": "^5.2.17",
    "postcss-cli": "^3.2.0",
    "postcss-csso": "^1.1.2",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.1.0",
    "source-map-support": "^0.5.0",
    "standard": "^10.0.3",
    "stylelint": "^8.0.0",
    "stylelint-config-suitcss": "^11.0.0",
    "suitcss-base": "^2.0.0",
    "suitcss-components-button": "^6.0.0",
    "suitcss-components-grid": "^3.0.3",
    "suitcss-utils-display": "^1.0.2",
    "suitcss-utils-flex": "^2.0.0",
    "suitcss-utils-layout": "^1.0.1",
    "suitcss-utils-link": "^1.0.1",
    "suitcss-utils-position": "^1.0.1",
    "suitcss-utils-size": "^2.0.0",
    "suitcss-utils-space": "^0.4.1",
    "suitcss-utils-text": "^1.0.0",
    "uglify-js": "^2.8.29",
    "uglifyify": "^3.0.4",
    "unassertify": "^2.0.4",
    "watchify-middleware": "^1.6.0",
    "yo-yoify": "^4.0.0"
  },
  "dependencies": {
    "body-parser": "~1.12.0",
    "chalk": "^2.1.0",
    "choo": "^6.5.0",
    "colors": "^1.1.2",
    "compression": "^1.7.0",
    "cookie-parser": "~1.3.3",
    "csv": "^0.4.6",
    "dateformat": "^1.0.12",
    "dedent": "^0.6.0",
    "dotenv": "^2.0.0",
    "escape-string-regexp": "^1.0.3",
    "express": "^4.15.4",
    "express-basic-auth": "^1.1.1",
    "express-session": "^1.15.5",
    "express-validator": "^2.10.0",
    "friendly-url": "^1.0.3",
    "fun-component": "^1.0.3",
    "got": "^7.1.0",
    "highcharts": "^5.0.14",
    "lodash.capitalize": "^4.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.merge": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "mapbox": "^1.0.0-beta9",
    "mapbox-gl": "^0.35.1",
    "method-override": "^2.3.9",
    "moment": "^2.18.1",
    "mongoose": "^4.11.12",
    "nanocomponent": "^6.4.2",
    "nanohref": "^3.0.0",
    "nodemailer": "^4.6.3",
    "object-hash": "^1.1.8",
    "passport": "^0.2.1",
    "passport-oauth2": "^1.4.0",
    "prismic-element": "^0.7.0",
    "prismic-javascript": "^1.5.0-beta.2",
    "prismic-richtext": "^0.2.7",
    "prismic.io": "^3.5.7",
    "y18n": "^3.2.1"
  }
}
